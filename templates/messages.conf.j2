Messages {
  Name = {{ bacula_messages_name }}
  mailcommand = "{{ bacula_messages_mail_command }}"
  operatorcommand = "{{ bacula_messages_operator_command }}"
  Mail = {{ bacula_messages_mail }} = error, warning, !skipped, !terminate, !restored
  append = "/var/log/bacula/info.log" = info, terminate
  append = "/var/log/bacula/error_warning.log" = error, warning, !terminate, notsaved
  append = "/var/log/bacula/all.log" = all
  operator = {{ bacula_messages_mail }} = mount
  console = error, warning, !terminate
  syslog = all
}


